  <body class="login">
    <div>
      <a class="hiddenanchor" id="signup"></a>
      <a class="hiddenanchor" id="signin"></a>
      <a class="hiddenanchor" id="lost"></a>

      <div class="login_wrapper">
        <div class="animate form login_form">
          <section class="login_content">
            <form action:"profile">
              <h1>Login Form</h1>
              <div class="field">
                <div class="control">
                  <input
                    type="text"
                    class="form-control"
                    placeholder="Username"
                    required=""
                  />
                </div>
              </div>
              <div class="field">
                <div class="control">
                  <input
                    type="password"
                    class="form-control"
                    placeholder="Password"
                    required=""
                  />
                </div>
              </div>

              <div>
                <button class="button is-medium submit">Log in</button>
                

                <a class="reset_pass to_register" href="#"
                  >Lost your password?</a
                >
              </div>

              <div class="clearfix"></div>

              <div class="separator">
                <p class="change_link">
                  New to site?
                  <a href="#signup" class="to_register"> Create Account </a>
                </p>

                <div class="clearfix"></div>
                <br />
              </div>
            </form>
          </section>
        </div>

        <div id="register" class="animate form registration_form">
          <section class="login_content">
            <form action="profile">
              <h1>Create Account</h1>
              <div>
                <input
                  type="text"
                  class="form-control"
                  placeholder="Username"
                  required=""
                />
              </div>
              <div>
                <input
                  type="email"
                  class="form-control"
                  placeholder="Email"
                  required=""
                />
              </div>
              <div>
                <input
                  type="password"
                  class="form-control"
                  placeholder="Password"
                  required=""
                />
              </div>
              <div>
                
                  <button class="button is-medium submit" href="index.html">
                    Submit
                  </button>

              </div>

              <div class="clearfix"></div>

              <div class="separator">
                <p class="change_link">
                  Already a member ?
                  <a href="#signin" class="to_register"> Log in </a>
                </p>

                <div class="clearfix"></div>
                <br />
              </div>
            </form>
          </section>
        </div>

